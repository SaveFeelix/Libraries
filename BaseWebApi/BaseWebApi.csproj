<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Title>BaseWebApi</Title>
        <Authors>SaveApis</Authors>
        <PackageProjectUrl>https://github.com/SaveFeelix/Libraries</PackageProjectUrl>
        <PackageLicenseUrl>https://www.apache.org/licenses/LICENSE-2.0.txt</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/SaveFeelix/Libraries</RepositoryUrl>
        <RepositoryType>GitHub</RepositoryType>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <AssemblyVersion>2.5.1</AssemblyVersion>
        <FileVersion>2.5.1</FileVersion>
        <Version>2.5.1</Version>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.5"/>
        <PackageReference Include="Microsoft.EntityFrameworkCore" Version="7.0.12"/>
    </ItemGroup>

</Project>
